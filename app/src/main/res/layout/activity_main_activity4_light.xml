<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity4Light">
    <ImageButton
        android:src="@drawable/ic_baseline_close_appday2"
        android:id="@+id/ib_close"
        android:background="@null"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="15dp"
        />
    <TextView
        android:id="@+id/tv_heading"
        android:textColor="@color/colortextlight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:text="manage profiles"
        android:layout_marginTop="15dp"
        android:textAllCaps="true"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/ib_close"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.17" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glvt_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5"
        />



    <ImageView
        android:id="@+id/iv_1"
        android:layout_width="100dp"
        android:layout_height="87dp"
        android:src="@drawable/textingimageicon"
        app:layout_constraintBottom_toTopOf="@id/tv_1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/glvt_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintTop_toTopOf="@id/gl_1"
        />

    <ImageView
        android:id="@+id/iv_2"
        android:layout_width="100dp"
        android:layout_height="87dp"
        android:src="@drawable/textingimageicon"
        app:layout_constraintBottom_toTopOf="@id/tv_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="@id/glvt_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/gl_1"
        />

    <TextView
        android:id="@+id/tv_1"
        style="@style/textmanageprofilelight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nguyen gia hy"
        app:layout_constraintBottom_toTopOf="@id/iv_3"
        app:layout_constraintTop_toBottomOf="@id/iv_1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/iv_2"
        android:text="nguyen giah y"
        style="@style/text2manaprofilelight"
        app:layout_constraintBottom_toTopOf="@id/iv_4"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/iv_3"
        android:layout_width="100dp"
        android:layout_height="87dp"
        android:src="@drawable/textingimageicon"
        app:layout_constraintBottom_toTopOf="@id/tv_3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/glvt_2"
        app:layout_constraintTop_toBottomOf="@id/tv_1"
        app:layout_constraintHorizontal_bias="0.5"
        />

    <ImageView
        android:id="@+id/iv_4"
        android:layout_width="100dp"
        android:layout_height="87dp"
        android:src="@drawable/textingimageicon"
        app:layout_constraintBottom_toTopOf="@id/tv_4"
        app:layout_constraintLeft_toLeftOf="@id/glvt_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_2"
        app:layout_constraintHorizontal_bias="0.5"/>

    <TextView
        android:id="@+id/tv_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hell"
        style="@style/textmanageprofilelight"
        app:layout_constraintBottom_toTopOf="@id/iv_5"
        app:layout_constraintTop_toBottomOf="@id/iv_3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_4"
        style="@style/text2manaprofilelight"
        android:text="pham minh tuan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/iv_6"
        app:layout_constraintTop_toBottomOf="@id/iv_4"
        tools:ignore="MissingConstraints"
        />
    <ImageView
        android:id="@+id/iv_5"
        android:layout_width="100dp"
        android:layout_height="87dp"
        android:src="@drawable/textingimageicon"
        app:layout_constraintBottom_toTopOf="@id/tv_5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/glvt_2"
        app:layout_constraintTop_toBottomOf="@id/tv_3"
        app:layout_constraintHorizontal_bias="0.5"
        />
    <ImageView
        android:id="@+id/iv_6"
        android:layout_width="100dp"
        android:layout_height="87dp"
        android:src="@drawable/textingimageicon"
        app:layout_constraintBottom_toTopOf="@id/tv_6"
        app:layout_constraintLeft_toLeftOf="@id/glvt_2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintTop_toBottomOf="@id/tv_4" />
    <TextView
        android:id="@+id/tv_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hello"
        style="@style/textmanageprofilelight"
        app:layout_constraintBottom_toBottomOf="@id/gl_2"
        app:layout_constraintTop_toBottomOf="@id/iv_5"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/tv_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="pham minh quoc"
        style="@style/text2manaprofilelight"
        app:layout_constraintBottom_toBottomOf="@id/gl_2"
        app:layout_constraintTop_toBottomOf="@id/iv_6"
        tools:ignore="MissingConstraints" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.91" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:orientation="horizontal"
        android:background="#D3D4D9"
        android:gravity="center"
        android:padding="5dp">
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:src="@drawable/ic_baseline_delete_appday2"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Delete User"
            android:textColor="#D95555"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>