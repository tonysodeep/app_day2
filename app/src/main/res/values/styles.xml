<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>
    <style name="textmanageprofileblack">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colortextchangeUserdark</item>
        <item name="android:textSize">20dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="layout_constraintRight_toRightOf">@id/glvt_2</item>
        <item name="layout_constraintHorizontal_bias">0.5</item>
    </style>
    <style name="textmanageprofilelight">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colortextlight</item>
        <item name="android:textSize">20dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="layout_constraintRight_toRightOf">@id/glvt_2</item>
        <item name="layout_constraintHorizontal_bias">0.5</item>
    </style>

    <style name="text2manaprofileblack">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colortextchangeUserdark</item>
        <item name="android:textSize">20dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="layout_constraintLeft_toLeftOf">@id/glvt_2</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
    </style>
    <style name="text2manaprofilelight">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colortextlight</item>
        <item name="android:textSize">20dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="layout_constraintLeft_toLeftOf">@id/glvt_2</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
    </style>
    <style name="textviewinleftwhite">
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/colortextchangeUserdark</item>
    </style>
    <style name="textviewinleftlight">
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/colortextlight</item>
    </style>
    <style name="textviewinleftblack">
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">#585454</item>
    </style>
    <style name="lineappday2back">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
    </style>
    <style name="switchinappday2">
        <item name="layout_constraintRight_toRightOf">parent</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="thumbTint">#FFFFFF</item>
        <item name="trackTint">#4AE0F5</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="closebt">
        <item name="android:layout_width">10dp</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:src">@drawable/ic_baseline_close_appday2</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
        <item name="android:background">@null</item>
    </style>
</resources>